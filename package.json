{
  "name": "@iteam/supreme",
  "version": "1.7.0",
  "main": "bin/supreme",
  "bin": "bin/supreme",
  "description": "A CLI tool that helps you get up an running fast",
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/ejs": "2.6.3",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.18",
    "@types/meow": "5.0.0",
    "husky": "3.0.4",
    "is-ci-cli": "1.1.1",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "tsdx": "0.9.0",
    "tslib": "1.10.0",
    "typescript": "3.6.2"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "ejs": "2.6.2",
    "execa": "2.0.4",
    "inquirer": "7.0.0",
    "meow": "5.0.0",
    "ora": "3.4.0",
    "read-pkg-up": "6.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}

{
  "name": "@iteam/supreme",
  "version": "2.9.1",
  "description": "A CLI tool that helps you get up an running fast",
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com",
    "url": "https://willcodefor.beer"
  },
  "contributors": [
    {
      "name": "Jimmy Jardland"
    }
  ],
  "repository": "github:Iteam1337/supreme",
  "homepage": "https://iteam1337.github.io/#/supreme",
  "bugs": "https://github.com/Iteam1337/supreme/issues",
  "main": "bin/supreme",
  "bin": "bin/supreme",
  "preferGlobal": true,
  "keywords": [
    "productivity",
    "cli",
    "generator"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint src test",
    "prepare": "npm run build",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@iteam/config": "12.1.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@types/app-root-path": "1.2.4",
    "@types/ejs": "2.6.3",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.18",
    "@types/meow": "5.0.0",
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "husky": "3.0.9",
    "is-ci-cli": "2.0.0",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "tsdx": "0.9.3",
    "tslib": "1.10.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "app-root-path": "2.2.1",
    "chalk": "2.4.2",
    "clipboardy": "2.1.0",
    "ejs": "2.7.1",
    "execa": "2.1.0",
    "inquirer": "7.0.0",
    "meow": "5.0.0",
    "ora": "4.0.2",
    "read-pkg-up": "7.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
